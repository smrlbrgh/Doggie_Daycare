<h1>Listing owners</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Primary phone</th>
      <th>Secondary phone</th>
      <th>Address</th>
      <th>City</th>
      <th>usstate</th>
      <th>Zip</th>
      <th>Emerg name</th>
      <th>Emerg phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @owners.each do |owner| %>
      <tr>
        <td><%= owner.first_name %></td>
        <td><%= owner.last_name %></td>
        <td><%= owner.primary_phone %></td>
        <td><%= owner.secondary_phone %></td>
        <td><%= owner.address %></td>
        <td><%= owner.city %></td>
        <td><%= owner.usstate %></td>
        <td><%= owner.zip %></td>
        <td><%= owner.emerg_name %></td>
        <td><%= owner.emerg_phone %></td>
        <td><%= link_to 'Show', owner %></td>
        <td><%= link_to 'Edit', edit_owner_path(owner) %></td>
        <td><%= link_to 'Destroy', owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Owner', new_owner_path %>
